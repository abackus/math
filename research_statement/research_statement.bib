@article{Dyatlov2016,
author={Dyatlov, Semyon
and Zahl, Joshua},
title={Spectral gaps, additive energy, and a fractal uncertainty principle},
journal={Geometric and Functional Analysis},
year={2016},
month={7},
day={01},
volume={26},
number={4},
pages={1011-1094},
abstract={We obtain an essential spectral gap for n-dimensional convex co-compact hyperbolic manifolds with the dimension {\$}{\$}{\{}{\backslash}delta{\}}{\$}{\$}of the limit set close to {\$}{\$}{\{}{\{}n-1{\backslash}over 2{\}}{\}}{\$}{\$}. The size of the gap is expressed using the additive energy of stereographic projections of the limit set. This additive energy can in turn be estimated in terms of the constants in Ahlfors--David regularity of the limit set. Our proofs use new microlocal methods, in particular a notion of a fractal uncertainty principle.},
issn={1420-8970},
doi={10.1007/s00039-016-0378-3},
url={https://doi.org/10.1007/s00039-016-0378-3}
}

